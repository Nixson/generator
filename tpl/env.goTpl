package gen

import (
	"embed"
	${controllerPath}${listenerPath}${migratePath}"github.com/Nixson/environment"
	"github.com/Nixson/lognx"
)

func InitMain(embedMain embed.FS) {
	environment.InitEnv(embedMain)
	if lognx.IfDebug() {
		lognx.Get().Debug("init ENV\t done")
	}
	${controllerInit}
	${listenerInit}
	${migrate}
}
